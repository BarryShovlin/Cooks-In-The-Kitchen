tchenProvider\"\nimport { KitchenCard } from \"../components/kitchens/KitchenCard\"\nimport { UserKitchenContext } from \"../components/kitchens/UserKitchenProvider\"\nimport { useHistory } from \"react-router-dom\"\n\n// import { PropsAndState } from './PropsAndState';\n\n\n\nexport const Home = () => {\n    const { kitchens, getKitchens } = useContext(KitchenContext)\n    const { addUserKitchen } = useContext(UserKitchenContext)\n    const history = useHistory()\n\n    useEffect(() => {\n        getKitchens()\n    }, [])\n\n    const handleClickAddKitchen = (event) => {\n        event.preventDefault()\n\n     \n        addUserKitchen(kitchens)\n            .then(() => history.push(\"/userKitchens\"))\n    }\n\n    return (\n        <>\n            <h1>Cooks In The Kitchen</h1>\n            <small>Keeping Consistency In The Kitchen</small>\n            <div>\n                <Link className=\"userKitchens\" to=\"/userKitchens\">Go to your kitchens</Link>\n            </div>\n            <div className=\"add_userKitchen\">\n                {\n                    kitchens.map(kitchen => {\n                        return <KitchenCard key={kitchen.id} kitchen={kitchen} />\n                    })\n                }\n            </div>\n\n        </>\n    );\n}","/Users/barry-shovlin/workspace/cooks_in_the_kitchen/src/components/nav/NavBar.js",[],"/Users/barry-shovlin/workspace/cooks_in_the_kitchen/src/components/kitchens/KitchenSelect.js",["163"],"/Users/barry-shovlin/workspace/cooks_in_the_kitchen/src/components/kitchens/KitchenProvider.js",[],"/Users/barry-shovlin/workspace/cooks_in_the_kitchen/src/components/kitchens/KitchenDetail.js",["164","165","166","167","168"],"/Users/barry-shovlin/workspace/cooks_in_the_kitchen/src/components/kitchens/KitchenCard.js",["169","170","171","172","173","174","175"],"import React, { useState, useContext, useEffect } from \"react\";\nimport \"./Kitchen.css\";\nimport { useHistory } from \"react-router-dom\"\nimport { KitchenContext }